{% extends 'layout.twig' %}

{% block feature %}
    <div class="box-feature">
      <div class="container">
        <h1 class="post-heightline-title">{{ post.head_banner_text }}</h1>
      </div>
    </div>
{% endblock %}

{% block content %}
  {% for post in posts %}
    {% include ["single-"~post.post_name~".twig", "single-"~post.post_type~".twig", 'tease.twig'] %}
  {% endfor %}
{% endblock %}

{% block aside %}

  {{ sidebar('sidebar-right') }}
  {{ flexible_content('group_sidebar')}}

{% endblock %}
